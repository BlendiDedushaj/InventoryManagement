@model InventoryManagement.Models.Currency

@{
    ViewData["Title"] = "Details";
}


<h1>Details - Currency</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ExchangeCurrencyId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ExchangeRate)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-secondary" asp-action="index"
       asp-route-pg="@TempData["CurrentPage"]">Back</a>
</div>